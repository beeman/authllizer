{
    "name": "authllizer-cordova-dialog",
    "version": "0.1.0",
    "description": "Authllizer Cordova Dialog",
    "main": "./es5/index.js",
    "browser": "./bundles/authllizer-cordova-dialog.umd.js",
    "es2015": "./es2015/index.js",
    "typings": "./typings/index.d.ts",
    "files": [
        "es5",
        "es2015",
        "bundles",
        "typings",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "run-s build:*",
        "build:es5": "tsc -t es5 -m commonjs --outDir es5",
        "build:es2015": "tsc -t es2015 -m es2015 --outDir es2015",
        "build:umd": "rollup -c ../../../config/rollup.config.ts",
        "clean": "rimraf es5 es2015 bundles typings",
        "prepublish": "run-s clean build",
        "watch": "run-p watch:*",
        "watch:es5": "npm run build:es5 -- --watch",
        "watch:es2015": "npm run build:es2015 -- --watch",
        "watch:umd": "npm run build:umd -- --watch"
    },
    "keywords": [
        "authllizer",
        "authllizer-dialog",
        "authentication",
        "auth",
        "oauth",
        "cordova",
        "phonegap"
    ],
    "author": {
        "email": "yisraelx@gmail.com",
        "name": "Yisrael Eliav",
        "url": "https://github.com/yisraelx"
    },
    "license": "MIT",
    "homepage": "https://github.com/yisraelx/authllizer#readme",
    "repository": {
        "type": "git",
        "url": "https://github.com/yisraelx/authllizer.git"
    },
    "bugs": {
        "url": "https://github.com/yisraelx/authllizer/issues"
    },
    "peerDependencies": {
        "@authllizer/core": "^0.1.0"
    },
    "devDependencies": {
        "@authllizer/core": "^0.1.0",
        "@types/cordova": "0.0.34"
    },
    "rollup": {
        "globals": {
            "@authllizer/core": "authllizer.core"
        }
    }
}
